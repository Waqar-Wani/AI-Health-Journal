{
    "name":  "kashmiri-health-logger",
    "version":  "1.0.0",
    "description":  "Personal Health Logger - Kashmiri Diet \u0026 Meds Dashboard",
    "main":  "server.js",
    "scripts":  {
                    "start":  "node server.js",
                    "dev":  "nodemon server.js",
                    "client":  "cd client \u0026\u0026 npm start",
                    "dev:full":  "concurrently \"npm run dev\" \"npm run client\"",
                    "build":  "npm run vercel-build",
                    "install:all":  "npm install \u0026\u0026 cd client \u0026\u0026 npm install",
                    "vercel-build":  "cd client \u0026\u0026 npm install \u0026\u0026 npm run build",
                    "prebuild":  "cd client \u0026\u0026 npm install"
                },
    "keywords":  [
                     "health",
                     "diet",
                     "medicine",
                     "kashmiri",
                     "dashboard"
                 ],
    "author":  "Health Logger Team",
    "license":  "MIT",
    "dependencies":  {
                         "express":  "^4.18.2",
                         "mongoose":  "^7.5.0",
                         "bcryptjs":  "^2.4.3",
                         "jsonwebtoken":  "^9.0.2",
                         "cors":  "^2.8.5",
                         "dotenv":  "^16.3.1",
                         "axios":  "^1.5.0",
                         "express-validator":  "^7.0.1",
                         "helmet":  "^7.0.0",
                         "morgan":  "^1.10.0",
                         "serverless-http":  "^3.2.0"
                     },
    "devDependencies":  {
                            "nodemon":  "^3.0.1",
                            "concurrently":  "^8.2.0"
                        }
}
